{
    "name": "m-adamski/symfony-helpers-bundle",
    "description": "Helpers Bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Adamski",
            "email": "m.adamski@ideacode.pl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "cakephp/chronos": "^1.1",
        "symfony/filesystem": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/orm-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "tecnickcom/tcpdf": "^6.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Adamski\\Symfony\\HelpersBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
